{
  "name": "@stun3r/acnh-translations",
  "version": "0.0.0",
  "scripts": {
    "build": "bash bin/build.sh",
    "prettier:fix": "prettier '**/*.ts' README.md --write",
    "start": "tsc -w",
    "commit": "git-cz",
    "release": "run-s version push",
    "release:minor": "run-s \"version -- --release-as minor\"",
    "release:major": "run-s \"version -- --release-as major\"",
    "version": "standard-version"
  },
  "author": "Thibaut DAVID",
  "license": "MIT",
  "devDependencies": {
    "@types/lodash": "^4.14.150",
    "@types/node": "^13.13.0",
    "commitizen": "^4.0.4",
    "cz-conventional-changelog": "3.1.0",
    "date-fns": "^2.12.0",
    "googleapis": "39",
    "lodash": "^4.17.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "quicktype": "^15.0.223",
    "standard-version": "^7.1.0",
    "ts-node": "^8.8.2",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
